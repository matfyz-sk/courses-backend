      "chalk": "^4.1.2",
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "express-fileupload": "^1.4.0",
      "express-http-proxy": "^1.6.3",
      "express-jwt": "^8.4.1",
      "express-validator": "^6.14.3",
      "jsonwebtoken": "^9.0.0",
      "lodash": "^4.17.21",
      "moment": "^2.29.4",
      "moment-timezone": "^0.5.40",
      "sparql-transformer": "^2.9.0",
      "virtuoso-sparql-client": "^0.2.2"
