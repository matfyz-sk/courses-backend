{
  "@context": {
  },
  "@graph": [{
    "@id": "?userID",
    "name": "?userName",
    "surname": "?userSurName",
    "email": "?userEmail",
    "teams": {
      "@id": "?teamID",
      "name": "?teamName"
    }
  }],
  "$where": [
    "?userID a courses:User",
    "?userID courses:name ?userName",
    "?userID courses:surname ?userSurName",
    "?userID courses:email ?userEmail",
    "?teamID courses:hasMember ?userID",
    "?teamID courses:name ?teamName"
  ],
  "$prefixes": {
    "courses": "http://www.courses.matfyz.sk/ontology#"
  }
}