{
  "@context": "http://schema.org/",
  "@graph": [{
    "@id" : "?id",
    "instanceOf": "?instance",
    "teamID": {
      "@id": "?teamID",
      "users": "?userID"
    }
  }],
  "$where": [
    "?id a courses:CourseInstance",
    "?id courses:instanceOf ?instance",
    "?teamID courses:course ?id",
    "?teamID courses:hasMember ?userID"
  ],
  "$prefixes": {
    "courses": "http://www.courses.matfyz.sk/ontology#"
  }
}
